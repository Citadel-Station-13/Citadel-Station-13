<ui-display title='Select' button>
	<ui-section>
		{{#each data.chemicals}}
			<ui-button grid icon='tint' state='{{data.current_reagent == id ? "selected" : null}}' action='select' params='{"reagent": "{{id}}"}'>{{title}}</ui-button>
	  	{{/each}}
	</ui-section>
	<ui-section>
		<span> Current volume: {{data.amount}} </span>
		<ui-button action='amount'>Change</ui-button><br>
  	</ui-section>
</ui-display>
