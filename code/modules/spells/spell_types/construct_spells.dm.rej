+	desc = "This spell dominates the mind of a lesser creature, causing it to see you as an ally."
+
+	charge_max = 600
+	range = 7
+	include_user = FALSE
+	selection_type = "range"
+	stat_allowed = FALSE
+
+	school = "evocation"
+	clothes_req = FALSE
+	invocation = "none"
