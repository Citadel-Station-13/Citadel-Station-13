+	name = "Abyssal Gaze"
+	desc = "This spell instills a deep terror in your target, temporarily chilling and blinding it."
+
+	charge_max = 750
+	range = 5
+	include_user = FALSE
+	selection_type = "range"
+	stat_allowed = FALSE
+
+	school = "evocation"
+	clothes_req = FALSE
+	invocation = "none"
+	invocation_type = "none"
